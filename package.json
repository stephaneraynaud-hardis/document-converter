{
  "name": "document-converter",
  "version": "0.1.0",
  "private": true,
  "bin": "server.js",
  "scripts": {
    "dev": "next dev --turbopack",
    "start": "next start",
    "lint": "next lint",
    "build": "npm run build:next && npm run build:fix && npm run build:pkg",
    "build:next": "next build",
    "build:fix": "sed -i '' 's/process.chdir(__dirname)//' .next/standalone/server.js",
    "build:pkg": "cd .next/standalone && pkg . --compress=GZip --sea",
    "open": "./out/next-pkg"
  },
  "pkg": {
    "assets": [
      ".next/**/*",
      "public/**/*.*"
    ],
    "targets": [
      "node22-macos-arm64"
    ],
    "outputPath": "../../out"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.38",
    "@ai-sdk/openai": "^2.0.54",
    "@ai-sdk/react": "^2.0.81",
    "@react-pdf/renderer": "^4.3.1",
    "@yao-pkg/pkg": "^6.12.0",
    "ai": "^5.0.81",
    "next": "^15.5.5",
    "pptxgenjs": "^4.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "typescript": "^5"
  }
}
